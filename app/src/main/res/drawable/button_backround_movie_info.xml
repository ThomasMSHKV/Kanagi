<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#A5A3A3"/>
            <gradient
                android:startColor="@color/white"
                android:endColor="@color/white"
                android:centerColor="#787272"/>
            <corners
                android:radius="35dp"/>
            <size
                android:height="70dp"
                android:width="300dp"/>


        </shape>
    </item>

    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#DFD5D5D2"/>
            <corners
                android:radius="35dp"/>
            <size
                android:height="70dp"
                android:width="300dp"/>


        </shape>
    </item>
</selector>